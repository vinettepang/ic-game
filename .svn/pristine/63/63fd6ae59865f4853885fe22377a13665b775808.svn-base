.btn.btn-active{
    position: relative;
    z-index: 10;
    display: block;
    color: #482929 !important;
    font-size: 24px;
    font-weight: bold;
    letter-spacing: 2.4px;
    width: 100%;
    height: 58px;
    line-height: 58px;
    text-align: center;
    background: #FFD733;
    border-radius: 100px;
    margin-left: 48px;
    margin-right: 48px;
    margin-top: 120px;
   /* margin-top: 112px;*/
    width: 90%;/*写给不支持calc()的浏览器*/ 
    width:-moz-calc(100% - 48px * 2); 
    width:-webkit-calc(100% - 48px * 2); 
    width: calc(100% - 48px * 2);
    border: none;
}
.btn.btn-active:hover,.btn.btn-active:visited,
.btn.btn-active:active{
    text-decoration: none;
    background: #FFD733;
}
